package com.kintiger.platform.order.pojo;

import java.math.BigDecimal;

import com.kintiger.platform.base.pojo.SearchInfo;

public class OrderList extends SearchInfo {

	private static final long serialVersionUID = 5879898578229881532L;

	//明细ID
	private Long list_id;
	
	//订单ID
	private Long order_id;
	
	//物料口径
	private String materiel_mvgr;
	
	//物料口径名称
	private String materiel_mvgr_name;
	
	//物料编码
	private String materiel_num;
	
	//物料名称
	private String materiel_name;
	
	//物料价格
	private BigDecimal materiel_price;
	//预打款=总金额-费用兑现
	private BigDecimal payMoney;
	
	//明细数量
	private BigDecimal list_count;
	
	//总金额=明细数量*物料价格
	private BigDecimal order_up_fund;
	
	//搭赠金额=搭赠数量*物料价格
	private BigDecimal order_give_fund;
	
	//搭赠数量
	private BigDecimal order_give_count;
	
	//费用兑现
	private BigDecimal order_use_fund;
	
	//明细类型(A,本品，B,搭赠)
	private String list_type;
	
	//明细状态
	private String list_state;
	
	//备注
	private String remark;
	//操作人
	private String userId;
	//送货地址
	private String address;

	public Long getList_id() {
		return list_id;
	}

	public void setList_id(Long list_id) {
		this.list_id = list_id;
	}

	public Long getOrder_id() {
		return order_id;
	}

	public void setOrder_id(Long order_id) {
		this.order_id = order_id;
	}

	public String getMateriel_num() {
		return materiel_num;
	}

	public void setMateriel_num(String materiel_num) {
		this.materiel_num = materiel_num;
	}

	public String getMateriel_name() {
		return materiel_name;
	}

	public void setMateriel_name(String materiel_name) {
		this.materiel_name = materiel_name;
	}

	public BigDecimal getMateriel_price() {
		return materiel_price;
	}

	public void setMateriel_price(BigDecimal materiel_price) {
		this.materiel_price = materiel_price;
	}

	public BigDecimal getList_count() {
		return list_count;
	}

	public void setList_count(BigDecimal list_count) {
		this.list_count = list_count;
	}

	public BigDecimal getOrder_up_fund() {
		return order_up_fund;
	}

	public void setOrder_up_fund(BigDecimal order_up_fund) {
		this.order_up_fund = order_up_fund;
	}

	public BigDecimal getOrder_give_fund() {
		return order_give_fund;
	}

	public void setOrder_give_fund(BigDecimal order_give_fund) {
		this.order_give_fund = order_give_fund;
	}

	public BigDecimal getOrder_use_fund() {
		return order_use_fund;
	}

	public void setOrder_use_fund(BigDecimal order_use_fund) {
		this.order_use_fund = order_use_fund;
	}

	public String getList_type() {
		return list_type;
	}

	public void setList_type(String list_type) {
		this.list_type = list_type;
	}

	public String getList_state() {
		return list_state;
	}

	public void setList_state(String list_state) {
		this.list_state = list_state;
	}

	public String getRemark() {
		return remark;
	}

	public void setRemark(String remark) {
		this.remark = remark;
	}

	public String getMateriel_mvgr() {
		return materiel_mvgr;
	}

	public void setMateriel_mvgr(String materiel_mvgr) {
		this.materiel_mvgr = materiel_mvgr;
	}

	public String getMateriel_mvgr_name() {
		return materiel_mvgr_name;
	}

	public void setMateriel_mvgr_name(String materiel_mvgr_name) {
		this.materiel_mvgr_name = materiel_mvgr_name;
	}

	public BigDecimal getOrder_give_count() {
		return order_give_count;
	}

	public void setOrder_give_count(BigDecimal order_give_count) {
		this.order_give_count = order_give_count;
	}

	public BigDecimal getPayMoney() {
		return payMoney;
	}

	public void setPayMoney(BigDecimal payMoney) {
		this.payMoney = payMoney;
	}

	public String getUserId() {
		return userId;
	}

	public void setUserId(String userId) {
		this.userId = userId;
	}

	public String getAddress() {
		return address;
	}

	public void setAddress(String address) {
		this.address = address;
	}
	
}
