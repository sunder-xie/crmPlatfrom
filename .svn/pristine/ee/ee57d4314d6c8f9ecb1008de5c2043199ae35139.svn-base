package com.kintiger.platform.supercheck.pojo;

import java.util.ArrayList;
import java.util.List;

import com.kintiger.platform.base.pojo.SearchInfo;
import com.kintiger.platform.framework.annotations.Decode;

public class AllitemConf extends SearchInfo {

    private static final long serialVersionUID = 1L;

    private Long confId;
   
    @Decode
    private String allitemName; // 综合品项系列
    
    private String phFlag="0"; // 
    private String clFlag="0"; // 
    private String hlFlag="0"; //
    private String jgFlag="0"; //
    private String allitems; // 单个品项
    private List<String> allitemList =new ArrayList<String>();
    private List<String> confidList =new ArrayList<String>();
    
    public Long getConfId() {
        return confId;
    }
    public void setConfId(Long confId) {
        this.confId = confId;
    }
    public String getAllitemName() {
        return allitemName;
    }
    public void setAllitemName(String allitemName) {
        this.allitemName = allitemName;
    }
    public String getPhFlag() {
        return phFlag;
    }
    public void setPhFlag(String phFlag) {
        this.phFlag = phFlag;
    }
    public String getClFlag() {
        return clFlag;
    }
    public void setClFlag(String clFlag) {
        this.clFlag = clFlag;
    }
    public String getHlFlag() {
        return hlFlag;
    }
    public void setHlFlag(String hlFlag) {
        this.hlFlag = hlFlag;
    }
    public String getJgFlag() {
        return jgFlag;
    }
    public void setJgFlag(String jgFlag) {
        this.jgFlag = jgFlag;
    }
    public String getAllitems() {
        return allitems;
    }
    public void setAllitems(String allitems) {
        this.allitems = allitems;
    }
    public List<String> getAllitemList() {
        return allitemList;
    }
    public void setAllitemList(List<String> allitemList) {
        this.allitemList = allitemList;
    }
    public List<String> getConfidList() {
        return confidList;
    }
    public void setConfidList(List<String> confidList) {
        this.confidList = confidList;
    }
    
   

}
