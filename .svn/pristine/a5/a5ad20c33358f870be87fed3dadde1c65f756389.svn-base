package com.kintiger.platform.sales.pojo;

import java.util.Date;

import com.kintiger.platform.base.pojo.SearchInfo;

/**
 * 物料组价格实体类
 * 
 */
public class A603Konp extends SearchInfo {
	private static final long serialVersionUID = -7492141504334102597L;

	private String vkorg;// 销售组织

	private String kondm;// 物料定价组

	private String kbetr;// 物料组价格

	private Date datab;// 有效起始日期

	private Date datbi;// 有效截止日期

	public String getVkorg() {
		return vkorg;
	}

	public void setVkorg(String vkorg) {
		this.vkorg = vkorg;
	}

	public String getKondm() {
		return kondm;
	}

	public void setKondm(String kondm) {
		this.kondm = kondm;
	}

	public String getKbetr() {
		return kbetr;
	}

	public void setKbetr(String kbetr) {
		this.kbetr = kbetr;
	}

	public Date getDatab() {
		return datab;
	}

	public void setDatab(Date datab) {
		this.datab = datab;
	}

	public Date getDatbi() {
		return datbi;
	}

	public void setDatbi(Date datbi) {
		this.datbi = datbi;
	}

}
