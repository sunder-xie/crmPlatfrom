package com.kintiger.platform.sales.pojo;

import com.kintiger.platform.base.pojo.SearchInfo;

/**
 * 物料回扣组实体类
 *
 */
public class Tvbot extends SearchInfo {
	private static final long serialVersionUID = -9206185908156614211L;
	
	private String mandt;// 客户端
	
	private String spras;// 语言代码
	
	private String bonus;// 物料回扣组
	
	private String vtext;// 描述
	
	private String flag;//经销商页面调用物料回扣组传K

	public String getMandt() {
		return mandt;
	}

	public void setMandt(String mandt) {
		this.mandt = mandt;
	}

	public String getSpras() {
		return spras;
	}

	public void setSpras(String spras) {
		this.spras = spras;
	}

	public String getBonus() {
		return bonus;
	}

	public void setBonus(String bonus) {
		this.bonus = bonus;
	}

	public String getVtext() {
		return vtext;
	}

	public void setVtext(String vtext) {
		this.vtext = vtext;
	}

	public String getFlag() {
		return flag;
	}

	public void setFlag(String flag) {
		this.flag = flag;
	}

}
