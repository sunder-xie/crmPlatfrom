package com.kintiger.platform.sms.pojo;

import com.kintiger.platform.base.pojo.SearchInfo;

public class SendInfo extends SearchInfo{

	private static final long serialVersionUID = 1116296957709177932L;
	private Integer id;
	private String mobile;//要发送的手机号码
	private String content;//要发送的短信内容
	private String smsNumber;//要发送的扩展短号（自动添加在公司短信号码后面）
	private Integer priority;//发送优先级，0 最高
	private String planTime;//短信定时发送时间，格式为 yyyy-mm-dd hh24:mi:ss， 如果不填写，表示马上发送
	private String sendType;//发送短信类型（如语音）
	public String getMobile() {
		return mobile;
	}
	public void setMobile(String mobile) {
		this.mobile = mobile;
	}
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
	public String getSmsNumber() {
		return smsNumber;
	}
	public void setSmsNumber(String smsNumber) {
		this.smsNumber = smsNumber;
	}
	public String getPlanTime() {
		return planTime;
	}
	public void setPlanTime(String planTime) {
		this.planTime = planTime;
	}
	public String getSendType() {
		return sendType;
	}
	public void setSendType(String sendType) {
		this.sendType = sendType;
	}
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public Integer getPriority() {
		return priority;
	}
	public void setPriority(Integer priority) {
		this.priority = priority;
	}
	
	
}
