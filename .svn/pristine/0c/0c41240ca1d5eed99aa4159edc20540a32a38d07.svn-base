package com.kintiger.platform.supercheck.pojo;

import java.util.ArrayList;
import java.util.List;

import com.kintiger.platform.base.pojo.SearchInfo;
import com.kintiger.platform.framework.annotations.Decode;

public class WeightAndPrice extends SearchInfo {

    private static final long serialVersionUID = 1L;

    private Long checkId;
    private String bezei40; // sap口味名称
    private String matg; // 口味编号

    @Decode
    private String checkChannelName; // 权重渠道
    private String phWeight; // 铺货权重
    private String clWeight; // 陈列权重
    private String price; // 品项价格
    
    private List<String> matgs = new ArrayList<String>();
    private List<String> prices =new ArrayList<String>();

    public String getMatg() {
	return matg;
    }

    public void setMatg(String matg) {
	this.matg = matg;
    }

    public String getBezei40() {
	return bezei40;
    }

    public void setBezei40(String bezei40) {
	this.bezei40 = bezei40;
    }

    public Long getCheckId() {
	return checkId;
    }

    public void setCheckId(Long checkId) {
	this.checkId = checkId;
    }

    public String getCheckChannelName() {
	return checkChannelName;
    }

    public void setCheckChannelName(String checkChannelName) {
	this.checkChannelName = checkChannelName;
    }

    public String getPhWeight() {
	return phWeight;
    }

    public void setPhWeight(String phWeight) {
	this.phWeight = phWeight;
    }

    public String getClWeight() {
	return clWeight;
    }

    public void setClWeight(String clWeight) {
	this.clWeight = clWeight;
    }

    public String getPrice() {
	return price;
    }

    public void setPrice(String price) {
	this.price = price;
    }

    public List<String> getMatgs() {
        return matgs;
    }

    public void setMatgs(List<String> matgs) {
        this.matgs = matgs;
    }

    public List<String> getPrices() {
        return prices;
    }

    public void setPrices(List<String> prices) {
        this.prices = prices;
    }

}
