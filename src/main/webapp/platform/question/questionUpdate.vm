<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=gbk" />
		#CSSM(["easyui","style"])
		#JSM(["jquery", "easyui","upload"])
		#JS_ENV_VAL()
	</head>
		<body class="easyui-layout">	
	<form  method="post" target="hideFrame" enctype="multipart/form-data"> 
           <div id="p_add" region="center" class="easyui-panel"
			title="问题基本信息" 
			data-options="iconCls:'icon-edit'">
          
						<table id="table_01" width="100%" cellspacing="0" border="0">
                                        <tr>
											<td class="head" nowrap>
												问题编号：
											</td>
											<td>
								                <input type="text"  id="qId"   name="q.qId" value="$!{q.qId}" class="easyui-validatebox"
								  readonly="true">
											</td>
											<td class="head" nowrap>
												提交时间：
											</td>
											<td>
												<input type="text"  id="qTime" value="$!{q.qTime}" class="easyui-validatebox"
								  readonly="true">
											</td>
                                            <td class="head" nowrap>
												组织：
											</td>
											<td>
											    <input type="text"  id="orgName" value="$!{q.orgName}" class="easyui-validatebox"
								  readonly="true">
											</td>
									 </tr>
									 <tr>
									       <td>&nbsp;</td>
									 </tr>
									 <tr>
                                            <td class="head" nowrap>
												提交人：
											</td>
											<td>
												<input type="text"  id="author" value="$!{q.author}" class="easyui-validatebox"
								  readonly="true">
											</td>
                                            <td class="head" nowrap>
												提交人联系方式：
											</td>
											<td>
												<input type="text"  id="phone" value="$!{q.phone}" class="easyui-validatebox"
								  readonly="true">
											</td>
											<td class="head" nowrap>
												提交人邮箱：
											</td>
											<td>
											    <input type="text"  id="authorEmail" name="authorEmail" value="$!{q.authorEmail}" class="easyui-validatebox"
								  readonly="true">
											</td>
									  </tr>
						      </table>
						<div class="easyui-panel" title="问题详细信息" data-options="collapsible:true,iconCls:'icon-list'" style="padding: 2px; background: #fafafa;">
						      <table width="100%" border="0">
									  <tr>
											<td class="head" nowrap>
												系统分类<font color='red'>*</font>：
											</td>
											<td> 
											    <input type="hidden" id="qTempVal" value="$!{q.qTemp}" />
												<select id="qTemp" name="q.qTemp">
												#*<option value="t200">SAP</option>
												<option value="t600">电子报销</option>
												<option value="t700">市场费用</option>
												<option value="t800">经销商管理</option>
												<option value="t900">手机系统</option>*#
												</select>
											</td>
                                            <td class="head" nowrap>
												问题分类<font color='red'>*</font>：
											</td>
											<td>
											    <input type="hidden" id="qTypeVal" value="$!{q.qType}" />
												<select id="qType" name="q.qType">
												#*<option value="p101">系统缺陷</option>
												<option value="p102">业务需求</option>
												<option value="p103">数据错误</option>
												<option value="p104">操作指导</option>
												<option value="p105">信息部工作任务</option>
												<option value="p106">权限</option>*#
												</select>
											</td>
											<td colspan="2">&nbsp;</td>
                                      </tr>
                                        <tr>
											<td class="head" nowrap>
												期望完成日期：
											</td>
											<td>
								               <input class="easyui-validatebox" id="expectDate" value='$!{q.expectDate}'  readonly="true" />
											</td>
                                           <td class="head" nowrap>
												问题等级<font color='red'>*</font>：
											</td>
											<td>
											    <input type="hidden" id="qLevelVal" value="$!{q.qLevel}" />
					                             <select id="qLevel" name="q.qLevel">
					                             <option value="">请选择</option>
					                             <option value="l101">非常紧急</option>
					                             <option value="l102">紧急</option>
					                             <option value="l103">一般</option>
					                             </select>
											</td>
											<td class="head" nowrap>
												处理状态<font color='red'>*</font>：
											</td>
											<td>
											    <input type="hidden" id="handleStateVal" value="$!{q.handleState}" />
								                 <select id="handleState" name="q.handleState">
					                             <option value="s101">请选择</option>
					                             <option value="s102">进行中</option>
					                             <option value="s103">已完成</option>
					                             </select>
											</td>
										</tr>
										<tr>
											<td class="head" nowrap>
												预计完成日期<font color='red'>*</font>：
											</td>
											<td>
								               <input class="easyui-datebox" id="lastDate" name="q.lastDate" value='$!{q.lastDate}' 
								                data-options="required:true,editable:false"></input>
											</td>
											<td class="head" nowrap>
												处理人<font color='red'>*</font>：
											</td>
										    <td>
											    <input type="text"  id="answerAuthor" name="q.answerAuthor" value="$!{q.answerAuthor}" class="easyui-validatebox"
								 readonly="true" />
											</td>
											<td colspan="2">
								                <a id="chooseAnswerAuthorBut" class="easyui-linkbutton" href="javascript:chooseAnswerAuthor();">处理人</a>
								                <input type="hidden" id="answerAuthorId" name="answerAuthorId" value="$!{q.answerAuthorId}" />
								                <input type="hidden" id="answerAuthorEmail" name="answerAuthorEmail" value="$!{q.answerAuthorEmail}" />
											   <a id="sendEmail" class="easyui-linkbutton" href="javascript:sendEmail();">发送邮件给处理人</a> 
											</td>
                                         </tr>
                                         <tr>
                                            <td class="head" nowrap>
												问题标题：
											</td>
											<td colspan="3">
												<textarea style="resize:none;" maxlength="100" cols="60" rows="1"  id="qTitle"   name="q.qTitle" class="easyui-validatebox"
								                  readonly="true">$!{q.qTitle}</textarea>
											</td>
                                         </tr>
                                         <tr>
                                         <td class="head" nowrap>
												问题描述：
											</td>
											<td colspan="4">
												<textarea maxlength="480" cols="80" rows="6"  id="question"   name="q.question" class="easyui-validatebox"
								  style="resize:none;" readonly="true">$!{q.question}</textarea>
											</td>
                                         </tr>
                                         <tr>
                                           <td class="head" noWrap>附件:</td>
						                   <td colspan="5">
							                 $!{q.link}
						                   </td>
                                         </tr>
                                         
                                         <tr>
                                            <td class="head" noWrap>附件上传:</td>
        				                    <td>
        				                    <div id="input">
        				                        <input name="upload" id="file0" type="file" value="添加附件" onchange="addFile()">
        				                    </div>
        				                    <div id="_file"></div>
        				                    </td>
                                         </tr>
                                  </table>
                       </div>
                      <div class="easyui-panel" title="处理结果" data-options="collapsible:true,iconCls:'icon-list'" style="padding: 2px; background: #fafafa;">
                                  <table id="communit_list" width="95%" border="0" cellpadding="0" cellspacing="1" class="outer"></table>
			                      <table width="100%" border="0" cellpadding="0" cellspacing="1">
			                          <input type="hidden" id="qId" name="qId" value="$!{qId}" />
			                          <input type="hidden" id="author" name="author" value="$!{author}" />
			                          <tr>
					                      <td width="10%" class="head" nowrap>回复：</td>
					                      <td width="20%">
						                    <textarea style="resize:none;" maxlength="216" cols="36" rows="6"  id="content"   name="content" 
						                     class="easyui-validatebox"></textarea>
					                      </td>
					                      <td width="30%">
					                        <font color="red">如果提交人对处理的结果还有问题请在此窗口回复信息，提出继续修改意见；如果处理人已处理完或处理过程出现问题，也在此处填写信息</font>
					                      </td>
					                      <td width="30%">&nbsp;</td>
                                      </tr>
			                      </table>
		              </div>
                       <div class="easyui-panel" title="用户确认评价" data-options="collapsible:true,iconCls:'icon-list'" style="padding: 2px; background: #fafafa;">
                         <table width="100%" border="0" cellpadding="1" cellspacing="1">
                            <tr>
                                        <td class="head" nowrap>
												处理速度（满分10分）：
										</td>
										<td>
								           <input type="text" size="1" maxlength="2" id="handleSpeed"   name="q.handleSpeed" value="$!{q.handleSpeed}" class="easyui-validatebox" readonly="true">
										   (1~10)
										</td>
										<td class="head" nowrap>
												处理结果（满分10分）：
										</td>
										<td>
										   <input type="text" size="1" maxlength="2"  id="handleResult"   name="q.handleResult" value="$!{q.handleResult}" class="easyui-validatebox" readonly="true">
										   (1~10)
										</td>
						   </tr>
						   <tr>
										<td class="head" nowrap>
												问题与建议：
											</td>
											<td>
												<textarea maxlength="216" cols="36" rows="6"  id="advice"   name="q.advice" class="easyui-validatebox"
								  style="resize:none;" readonly="true">$!{q.advice}</textarea>
											</td>
											<td>
											    <font color="red">提交人对处理人处理结果已经没问题，填写最终评语信息</font>
											</td>
							</tr>
                         </table>
                      </div>
		                          <div region="south" border="false" style="text-align:right;height:30px;line-height:30px;">
				                      <a class="easyui-linkbutton" icon="icon-ok" href="javascript:updateQuestion()">提交</a>
				                      <a class="easyui-linkbutton" icon="icon-cancel" href="javascript:cencel()">取消</a>
		                          </div>
                               
            </div>
	
		 </form>
           <iframe id="hideFrame" name="hideFrame"  style="height: 0; width: 0"></iframe>
           <div id="maintWindow"/>
           	</body>
         <script type="text/javascript" src="$!{env.imgUrl}/js/platform/question/questionUpdate.js?version=20170228"></script>
</html>