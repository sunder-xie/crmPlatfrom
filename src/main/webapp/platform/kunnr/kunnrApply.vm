					#foreach($kunnrLicense in $kunnrLicenseList)
					#set($velocityCount=$velocityCount+1)
                	<tr>
                		<td class="head" noWrap>$!{kunnrLicense.licenseName}:</td>
                    	<td>
                    	<input type="hidden" name="licenseName" value="$!{kunnrLicense.licenseName}"></input>
                		<input type="file" name="license">
                		<input type="hidden" id="oldLice_$velocityCount" value="$!{kunnrLicense.licenseName}"></input>
                		<a href="$!{kunnrLicense.licensePath}" target="_blank">$!{kunnrLicense.licenseFile}</a>
                		</td>
                		<td class="head" noWrap>证照有效期:</td>
                		<td>
                		<input class="easyui-datebox" name="licenseValid" value="$!dateUtil.datetime($!{kunnrLicense.licenseValid},'yyyy-MM-dd')" data-options="editable:false"/></td>
                	</tr>
					#end
		    		</table> 
				</div>
				#else
				<div id="panel" class="easyui-panel" title="证照上传" data-options="collapsible:false,iconCls:'icon-detail'" style="padding: 2px; background: #fafafa;">
			    <!--<div title="证照上传" style="overflow:auto;padding:5px;">-->  
				</div>
				#end
			</br>
			#if($!{userId}==$!{kunnr.createUserId})
			<div  class="saveBut" border="false" style="text-align:center;height:30px;line-height:30px;">
			    <a class="easyui-linkbutton" icon="icon-ok" href="javascript:save()">存草稿</a>
			    <a class="easyui-linkbutton" icon="icon-ok" href="javascript:submit()">提交</a>
				<a class="easyui-linkbutton" icon="icon-cancel" href="javascript:cancel()">重置</a>
			</div>
			#end
			<iframe id="hideFrame" name="hideFrame"  style="height: 0; width: 0"></iframe>
		</form>
		<div id="nextUserDialog"/>
	</body>
	#JSM(["jquery", "easyui"])
	#JS_ENV_VAL()
	<script type="text/javascript" src="$!{env.imgUrl}/js/platform/kunnr/kunnrApply.js?version=20170303"></script>
</html>