	<form id="fm" method="post">
		<div class="fitem">
			<label>综合品项名称:</label>
			<input name="allitemName" class="easyui-validatebox" required="true">
		</div>

		
	</form>
</div>
<div id="dlg-buttons">
	<a href="#" class="easyui-linkbutton" iconCls="icon-ok" onclick="submitAllitemConf()">提交</a>
	<a href="#" class="easyui-linkbutton" iconCls="icon-cancel" onclick="javascript:$('#dlg').dialog('close')">取消</a>
</div>
		
