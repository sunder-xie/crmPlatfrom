	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=gb2312" />
		#CSSM(["easyui","style"])
		#JSM(["jquery", "easyui", "upload"])
		#JS_ENV_VAL()	
	</head>
	<body>
		<form id="ff" method="post" enctype="multipart/form-data" target="hideFrame">
			
			<input type="hidden" id="defaultUserId" name="defaultUserId" value="$!{allUsers.userId}" />
			<input type="hidden" id="defaultCompanyCode" name="defaultCompanyCode" value="$!{companyCode}" />
			
		<div id="dd" class="easyui-panel" title="考核权重&价格配置" data-options="collapsible:true,iconCls:'icon-search'" style="padding: 8px">
				<table width="100%" border="0" cellpadding="5" cellspacing="1">
					<tr>
					   
						<td>
							&nbsp;&nbsp;&nbsp;
							<a href="javascript:addAllitemConf()" class="easyui-linkbutton" iconCls="icon-search">添加综合品项</a>&nbsp;
