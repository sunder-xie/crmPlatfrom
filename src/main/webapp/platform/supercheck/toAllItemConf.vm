							<a href="javascript:excelDownload()" class="easyui-linkbutton" iconCls="icon-search">下载</a>&nbsp;
							<a href="javascript:save()" class="easyui-linkbutton" iconCls="icon-reload">保存</a>
						</td>
						</tr>
				</table>
			</div>
		
			<div border="false" style="height:25px;"></div>
		
			
			<table id="myTab" width="98%">
			</table>
		</form>
		
	 <div id="dlg" class="easyui-dialog" style="width:400px;height:120px;padding:10px 20px"
		closed="true" buttons="#dlg-buttons">
